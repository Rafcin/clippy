{
  "private": false,
  "license": "Apache-2.0",
  "version": "0.0.0",
  "name": "@oxygen/llm",
  "description": "This package is a collection of tools for working with the OpenAI API, and developing plugins for ChatGPT.",
  "author": {
    "name": "Raf Szuminski",
    "email": "rafcin.s@gmail.com",
    "url": "https://rafszuminski.com"
  },
  "repository": "rafcin/oxygen.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "openai",
    "api",
    "gpt",
    "puppeteer",
    "langchain"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "python",
    "README.md"
  ],
  "scripts": {
    "[--- Build ---]": "",
    "build": "tsup",
    "[--- Dev ---]": "",
    "dev": "pnpm build --watch",
    "[--- Lint ---]": "",
    "lint": "tsc --noEmit --project tsconfig.dev.json && pnpm eslint ./src --ext .js,.ts,.tsx",
    "[--- Clean ---]": "",
    "clean": "pnpm rimraf dist/ node_modules/ .turbo/"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.21.0",
    "axios": "^1.3.6",
    "cheerio": "1.0.0-rc.12",
    "cors": "^2.8.5",
    "crawlee": "^3.3.1",
    "esbuild-plugin-raw": "^0.1.7",
    "googlebard": "^1.0.4",
    "gpt3-tokenizer": "^1.1.5",
    "langchain": "^0.0.59",
    "node-fetch": "^3.3.1",
    "openai": "^3.2.1",
    "parse-numeric-range": "^1.3.0",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^19.10.0",
    "puppeteer-core": "^19.10.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "turndown": "^7.1.2",
    "url-parse": "^1.5.10"
  }
}
